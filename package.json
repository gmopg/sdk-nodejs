  "nyc": {
    "check-coverage": true,
    "include": [
      "lib/**/*.js"
    ],
    "reporter": [
      "lcov",
      "text-summary"
    ]
  },
  "name": "@motionpicture/gmo-service",
  "repository": {},
  "scripts": {
    "build": "tsc",
    "check": "npm run tslint && nsp check && npm run snyk",
